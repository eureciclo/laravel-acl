{
  "name": "epscavalcante/laravel-acl",
  "description": "Advanced ACL for Laravel",
  "keywords": [
    "laravel",
    "acl",
    "groups",
    "roles"
  ],
  "homepage": "https://github.com/epscavalcante/laravel-acl",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo",
      "email": "eduardo.ps.cavalcante@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^10.5.0",
    "orchestra/testbench": "^9.0.0",
    "squizlabs/php_codesniffer": "3.6.0",
    "phpmd/phpmd" : "^2.15.0",
    "mockery/mockery": "^1.6.7"
  },
  "require": {
    "php": "^8.3",
    "illuminate/support": "^11",
    "illuminate/container": "^11",
    "illuminate/database": "^11",
    "illuminate/events": "^11",
    "laravel/helpers": "1.7.0"
  },
  "autoload": {
    "psr-4": {
      "Signes\\Acl\\": "src/Acl/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "src/Publish"
    ],
    "psr-4": {
      "Signes\\Acl\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
